// Test break and continue in loops

fn main() -> i32 {
    let total = 0

    // Test break: exit loop early
    for i in 0..10 {
        if i == 5 {
            break
        }
        total = total + i
    }

    // total should be 0+1+2+3+4 = 10

    // Test continue: skip some iterations
    for i in 0..5 {
        if i == 2 {
            continue
        }
        total = total + i
    }

    // total should be 10 + (0+1+3+4) = 10 + 8 = 18
    return total
}
