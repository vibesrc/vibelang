// Test array literal type coercion with all explicit types

fn main() -> i32 {
    // Array with explicit i32 element type
    let arr: i32[4] = [5, 10, 15, 20]

    // Explicitly typed accumulator
    let total: i32 = 0
    for x in arr {
        total = total + x
    }

    return total   // Should return 50
}
