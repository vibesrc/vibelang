// Factorial - tests functions, recursion, and return values

fn factorial(n: i64) -> i64 {
    if n <= 1 {
        return 1
    }
    return n * factorial(n - 1)
}

fn main() {
    print("Factorials 1-10:")
    let i = 1
    while i <= 10 {
        let result = factorial(i)
        print_int(result)
        i = i + 1
    }
}
