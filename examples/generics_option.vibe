// Test generic enum Option<T>

enum Option<T> {
    Some(T),
    None
}

fn unwrap_or<T>(opt: Option<T>, default: T) -> T {
    match opt {
        Option.Some(val) => val,
        Option.None => default
    }
}

fn main() -> i32 {
    let x = Option<i32>.Some(42)
    let y = Option<i32>.None
    let a = unwrap_or<i32>(x, 0)
    let b = unwrap_or<i32>(y, 99)
    return a + b    // should be 42 + 99 = 141
}
