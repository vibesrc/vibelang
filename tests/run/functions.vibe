// Test function definitions and calls

fn add(a: i64, b: i64) -> i64 {
    return a + b
}

fn multiply(x: i64, y: i64) -> i64 {
    return x * y
}

fn factorial(n: i64) -> i64 {
    if n <= 1 {
        return 1
    }
    return n * factorial(n - 1)
}

fn fibonacci(n: i64) -> i64 {
    if n <= 1 {
        return n
    }
    return fibonacci(n - 1) + fibonacci(n - 2)
}

fn no_return() {
    let x = 1
}

fn early_return(x: i64) -> i64 {
    if x < 0 {
        return -1
    }
    if x == 0 {
        return 0
    }
    return 1
}

fn main() -> i64 {
    // Simple function calls
    if add(3, 5) != 8 {
        return 1
    }

    if multiply(4, 7) != 28 {
        return 2
    }

    // Recursive function
    if factorial(5) != 120 {
        return 3
    }

    if fibonacci(10) != 55 {
        return 4
    }

    // Nested calls
    let result = add(multiply(2, 3), multiply(4, 5))
    if result != 26 {
        return 5
    }

    // Early return
    if early_return(-5) != -1 {
        return 6
    }
    if early_return(0) != 0 {
        return 7
    }
    if early_return(10) != 1 {
        return 8
    }

    // Void function
    no_return()

    return 0
}
