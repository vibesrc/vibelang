// Conformance test: arithmetic operations
// Should compile without errors

fn add(a: i32, b: i32) -> i32 {
    return a + b
}

fn sub(a: i32, b: i32) -> i32 {
    return a - b
}

fn mul(a: i32, b: i32) -> i32 {
    return a * b
}

fn div(a: i32, b: i32) -> i32 {
    return a / b
}

fn main() {
    let x = add(10, 20)
    let y = sub(30, 10)
    let z = mul(5, 6)
    let w = div(100, 4)
}
